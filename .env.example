# Database
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_api_key

# Google Drive API (Required for Google Drive integration)
GOOGLE_DRIVE_CLIENT_ID=your_google_drive_client_id_here
GOOGLE_DRIVE_CLIENT_SECRET=your_google_drive_client_secret_here
GOOGLE_DRIVE_REDIRECT_URI=http://localhost:3000/auth/google-drive/callback

# LLM Provider
OPENAI_API_KEY=your_openai_key
ANTHROPIC_API_KEY=your_anthropic_key
LLM_PROVIDER=anthropic
LLM_MODEL=claude-3-haiku-20240307

# Embedding Provider (Note: Anthropic doesn't provide embeddings, so we use OpenAI)
EMBEDDING_MODEL=text-embedding-3-small

# Application
PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# MCP Server
MCP_SERVER_PORT=3001
MCP_SERVER_HOST=localhost

# Security
JWT_SECRET=your_jwt_secret
CORS_ORIGIN=http://localhost:3000

# File Processing
MAX_FILE_SIZE=10485760
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
